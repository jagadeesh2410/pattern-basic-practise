//pattern

#include <stdio.h>

void main() {
    // Write C code here
    int n=3;
    for(int i=1;i<=n;i++){
        int temp=n;
        for (int j=1;j<=i;j++){
            printf("%d ",temp--);
        }
        printf("\n");
    }

  
}


// Online C compiler to run C program online
#include <stdio.h>

void main() {
   
    int n=5;
    for(int i=1;i<=n;i++){
        int temp=1;
        for (int j=1;j<n;j++){
           
          if(j<=n-i){
          printf ("  ");
          }
          else{
             printf("%d ",temp++);
           }
        }
        int t=i;
        for(int j=1;j<=n;j++){
            if(j<=i)
            printf ("%d ",t--);
        }
       
          
        printf("\n");
    }

  
}
#include <stdio.h>

void main() {
   
    int n=5;
         for(int i=1;i<=n;i++){
       
        for (int j=1;j<n;j++){
           printf ("%d ",n-i+1) ;
        }
       
       
          
        printf("\n");
    }

  
} 
